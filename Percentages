def fifteenPercent(billAmount):
    fifteenPercent = .15
    totalBill15 = billAmount + fifteenPercent


def eighteenPercent(billAmount):
    eighteenPercent = .18
    totalBill18 = billAmount + eighteenPercent


def twentyPercent(billAmount):
    twentyPercent = .20
    totalBill20 = billAmount + twentyPercent

